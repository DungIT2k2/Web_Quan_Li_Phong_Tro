{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        },
        {
            "src": "/room",
            "dest": "src/routes/room.js"
        },
        {
            "src": "/utility",
            "dest": "src/routes/utility.js"
        },
        {
            "src": "/payment",
            "dest": "src/routes/payment.js"
        },
        {
            "src": "/account",
            "dest": "src/routes/account.js"
        },
        {
            "src": "/manage",
            "dest": "src/routes/manage.js"
        },
        {
            "src": "/user",
            "dest": "src/routes/user.js"
        },
        {
            "src": "/",
            "dest": "src/routes/login.js"
        }
    ]
}