{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "src/routes/room.js",
            "src": "/room"
        },
        {
            "handle": "src/routes/utility.js",
            "src": "/utility"
        },
        {
            "handle": "src/routes/payment.js",
            "src": "/payment"
        },
        {
            "handle": "src/routes/account.js",
            "src": "/account"
        },
        {
            "handle": "src/routes/user.js",
            "src": "/user"
        },
        {
            "handle": "src/routes/login.js",
            "src": "/"
        }
    ]
}